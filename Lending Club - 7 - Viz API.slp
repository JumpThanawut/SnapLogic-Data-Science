                        "view_type": {
                            "value": "document"
                        }
                    }
                },
                "settings": {
                    "editable_content": {
                        "value": "from snaplogic.tool import SLTool as slt\n\nslt.ensure(\"bokeh\", \"0.13.0\")\n\n# Imports\nimport os\nfrom collections import Counter\nimport pandas as pd\nfrom bokeh.io import output_file, save\nfrom bokeh.models import ColumnDataSource, FactorRange\nfrom bokeh.transform import factor_cmap\nfrom bokeh.plotting import figure\nfrom bokeh.palettes import Category20_20\n\n# Global Variables\nnum_bin = 10\nclass_field = \"loan_status\"\ndisplay_class_field = \"Loan Status\"\ndf = None\nrequest_queue = []\nviz_cache = {}\ntmp_path = os.path.join(tmp_root, \"viz.html\")\noutput_file(tmp_path)\n\n\n# This function will be executed once before consuming the data.\ndef snaplogic_init():\n    return None\n\n\n# This function will be executed on each document from the upstream snap.\ndef snaplogic_process(row):\n    global num_bin\n    global class_field\n    global display_class_field\n    global df\n    global request_queue\n    global viz_cache\n    output_list = []\n\n    if \"group\" in row:\n        # Load dataset and convert into DataFrame.\n        df = pd.DataFrame(row[\"group\"])\n    else:\n        request_queue.append(row)\n\n    if df is not None:\n        while len(request_queue) > 0:\n            try:\n                request = request_queue.pop(0)\n                original_field = request[\"original_field\"]\n                display_field = request[\"display_field\"]\n                field_type = request[\"type\"]\n                options = request[\"options\"]\n\n                if original_field not in viz_cache:\n                    viz_cache[original_field] = generate_viz(df, original_field, display_field, field_type, options,\n                                                             class_field, display_class_field, num_bin)\n\n                output_list.append({\"viz\": viz_cache[original_field]})\n            except:\n                output_list.append({\"viz\": \"The request is not valid.\"})\n\n    if \"group\" in row:\n        # Do not output if the input document is a dataset. This is needed in order to preserve lineage property.\n        output_list.append(slt.get_drop_doc())\n\n    return output_list\n\n\n# This function will be executed after consuming all documents from the upstream snap.\ndef snaplogic_final():\n    return None\n\n\n# Generate visualization based on the input field.\ndef generate_viz(df, original_field, display_field, field_type, options,\n                 class_field, display_class_field, num_bin):\n    class_set = df[class_field].unique()\n\n    # Plot numeric field.\n    if field_type == \"number\":\n        max = df[original_field].max()\n        min = df[original_field].min()\n        bin_size = (max - min) / num_bin\n        num_class = len(class_set)\n\n        class_bin_counter_map = {}\n        class_bin_freq_list_map = {}\n\n        for val in class_set:\n            bin_counter = Counter()\n            for x, y in zip(df[original_field], df[class_field]):\n                if y != val:\n                    continue\n                bin = int((x - min) / bin_size)\n                bin_counter[bin if bin != num_bin else num_bin - 1] += 1\n\n            class_bin_counter_map[val] = bin_counter\n            bin_freq_list = []\n            for i in range(0, num_bin):\n                bin_freq_list.append(bin_counter[i])\n            class_bin_freq_list_map[val] = bin_freq_list\n\n        p = figure(x_axis_label=display_class_field, y_axis_label=display_field,\n                   x_range=class_set, plot_width=600, plot_height=580)\n        for val in class_set:\n            p.vbar([val] * num_bin,\n                   top=[min + i * bin_size for i in range(1, 11)],\n                   bottom=[min + i * bin_size for i in range(0, 10)],\n                   width=0.7, line_color=None,\n                   fill_alpha=[x / len(df) * num_class for x in class_bin_freq_list_map[val]])\n        p.y_range.start = options[0]\n\n    # Plot categorical field.\n    elif field_type == \"dropdown\":\n        field_unique = sorted(df[original_field].unique())\n        counter = Counter()\n        for x, y in zip(df[original_field], df[class_field]):\n            counter[(y, x)] += 1\n        x = [(c, v) for c in class_set for v in field_unique]\n        source = ColumnDataSource(data=dict(x=x, counts=[counter[point] for point in x]))\n        p = figure(x_axis_label=display_class_field, y_axis_label=\"Count\", plot_width=600, plot_height=580,\n                   x_range=FactorRange(*x))\n        p.vbar(x=\"x\", top=\"counts\", width=0.9, source=source, line_color=None,\n               fill_color=factor_cmap(\"x\", palette=Category20_20 * 10, factors=field_unique, start=1, end=2))\n        p.y_range.start = 0\n        p.x_range.range_padding = 0.1\n        p.xaxis.major_label_orientation = 1\n        \n    save(p)\n    with open(tmp_path, \"r\") as tmp_file:\n        viz_html = tmp_file.read()\n\n    return viz_html"
                    }
                },
                "view_serial": 100
            }
        }
